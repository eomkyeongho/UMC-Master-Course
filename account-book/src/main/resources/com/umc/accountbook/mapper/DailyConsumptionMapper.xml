<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.umc.accountbook.mapper.DailyConsumptionMapper">
    <select id="getDailyConsumption" parameterType="String" resultType="com.umc.accountbook.domain.DailyConsumption">
        select * from
            daily_consumption
        where user_id = #{user_id}
          and date = #{date}
    </select>
</mapper>